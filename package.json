{
  "name": "oauth-discord",
  "version": "1.4.5",
  "description": "simple discord oauth tool",
  "main": "./src/index.js",
  "types": "./typings/index.d.ts",
  "exports": {
    ".": [
      {
        "require": "./src/index.js",
        "import": "./esm/oauth.mjs"
      },
      "./src/index.js"
    ],
    "./esm": "./esm/oauth.mjs"
  },
  "scripts": {
    "test": "npm run lint && npm run lint:typings",
    "lint": "eslint src",
