    "lint:fix": "eslint src --fix"
  },
  "author": "sujang958",
  "license": "MIT",
  "dependencies": {
    "btoa": "^1.2.1",
    "node-fetch": "^2.6.1"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^4.22.1",
    "@typescript-eslint/parser": "^4.22.1",
    "eslint": "^7.25.0",
    "typescript": "^4.2.4"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sujang958/oauth-discord.git"
  },
  "bugs": {
    "url": "https://github.com/sujang958/oauth-discord.git/issues"
  },
  "homepage": "https://github.com/sujang958/oauth-discord.git#readme",
  "engines": {
    "node": ">=12.0.0"
